{
  "name": "raddaten-hackathon",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "webpack": "webpack",
    "deploy": "npm run webpack && npm run optimize:inline",
    "deploy:local": "npm run clean && npm run webpack && npm run optimize:inline",
    "before": "npm install",
    "copy:html": "npm -v",
    "copy:html2": "cp -r src/app/dashboard/index.html dist/",
    "clean": "rm -rf dist/ ",
    "optimize:inline": "node -v",
    "optimize:inline2": "node scripts/optimize.js",
    "start": "npm run deploy & webpack serve  --port 4200",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "tsc",
    "test": "jest --env=jsdom --coverage --runInBand --verbose",
    "test:clear": "jest --clearCache",
    "test:log": "jest --env=jsdom --verbose false",
    "test:watch": "jest --env=jsdom --watchAll  --coverage --runInBand --unhandled-rejections=strict ",
    "clear:jest": "jest --clearCache",
    "make-badges": "coverage-badges --output coverage/badges.svg",
    "prepublish": "npm run test && npm run make-badges",
    "run:backend": "ts-node src/app/backend/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/jquery": "^3.5.14",
    "@types/node": "^17.0.35",
    "@types/request": "^2.48.8",
    "@types/testing-library__dom": "^7.5.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vercel/ncc": "^0.34.0",
    "axios": "^1.3.6",
    "coverage-badges-cli": "^1.1.1",
    "eslint": "^8.20.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "extra-watch-webpack-plugin": "^1.0.3",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-fetch-mock": "^3.0.3",
    "jsonminify": "^0.4.2",
    "node-fetch": "^3.3.1",
    "prettier": "^2.2.1",
    "ts-jest": "^29.0.5",
    "ts-loader": "^8.0.17",
    "ts-node": "^10.8.1",
    "typescript": "^4.6.3",
    "webpack": "^5.22.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^4.13.1",
    "webpack-shell-plugin-next": "^2.2.2",
    "whatwg-fetch": "^3.6.2"
  },
  "dependencies": {
    "https": "^1.0.0",
    "request": "^2.88.2"
  }
